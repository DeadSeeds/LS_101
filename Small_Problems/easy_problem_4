vehicles = ['car', 'car', 'truck', 'car', 'SUV', 'truck', 'motorcycle', 'motorcycle', 'car', 'truck']

def count_occurrences(vehicles)
  new_hash = {}
  vehicles.each do |vehicle|
    if new_hash[vehicle]
      new_hash[vehicle] += 1
    else
      new_hash[vehicle] = 1
    end
  end
  new_hash
end

count_occurrences(vehicles)
